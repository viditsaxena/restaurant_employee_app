<h3>Item Orders</h3>

<div class="row">
<table class="" id="chef-view">
  <thead>

    <% @parties.each do |party| %>
    <tr>
       <th><%= party.stand_number%></th>
    </tr>
  </thead>
   <tbody>
     <% party.item_orders.each do |item_order| %>
     <tr>
       <td><%= item_order.dish.name %></td>
       <td class="time-since"><%= ((@time_now - item_order.created_at)/1.hour)%></td>
       <td class="hide_on_click clickable">
         <a class="btn-floating time-action">
           <i class="material-icons">thumb_up</i>
         </a>
       </td>
     </tr>
     <% end %>

   </tbody>
   <% end %>
 </table>
</div>

